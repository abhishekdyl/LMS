<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/assignment_review/db/" VERSION="20221017" COMMENT="XMLDB file for core Moodle tables"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">

    <TABLES>
    <TABLE NAME="curl_response" COMMENT="The stored gptzero api responses">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="true" />
        <FIELD NAME="sid" TYPE="char" LENGTH="30" NOTNULL="false" SEQUENCE="false" />
        <FIELD NAME="user_id" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" />
        <FIELD NAME="response" TYPE="text" NOTNULL="false" SEQUENCE="false" />
        <FIELD NAME="type" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" />
        <FIELD NAME="text_string" TYPE="text" NOTNULL="false" SEQUENCE="false" />
        <FIELD NAME="status" TYPE="char" LENGTH="30" NOTNULL="false" SEQUENCE="false" />
        <FIELD NAME="status_code" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>

    </TABLE>
  </TABLES>

</XMLDB>