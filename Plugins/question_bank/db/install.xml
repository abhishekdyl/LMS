<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/question_bank/db" VERSION="20161026" COMMENT="XMLDB file for Moodle local/question_bank"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_qbquestions" COMMENT="question_bank completion cm settings">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="questionid" TYPE="int" LENGTH="20" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="status" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT="0" SEQUENCE="false" />
        <FIELD NAME="courseid" TYPE="int" LENGTH="20" NOTNULL="true" DEFAULT="0" SEQUENCE="false" />
        <FIELD NAME="coursecontextid" TYPE="int" LENGTH="20" NOTNULL="true" DEFAULT="0" SEQUENCE="false" />
        <FIELD NAME="createdby" TYPE="int" LENGTH="20" NOTNULL="true" DEFAULT="0" SEQUENCE="false" />
        <FIELD NAME="createdtime" TYPE="int" LENGTH="20" NOTNULL="true" DEFAULT="0" SEQUENCE="false" />
        <FIELD NAME="modifiedby" TYPE="int" LENGTH="20" NOTNULL="false" SEQUENCE="false" />
        <FIELD NAME="modifiedtime" TYPE="int" LENGTH="20" NOTNULL="false" SEQUENCE="false" />
        <FIELD NAME="approveby" TYPE="int" LENGTH="20" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="approvetime" TYPE="int" LENGTH="20" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>